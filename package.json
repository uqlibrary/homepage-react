    "scripts": {
        "analyse": "webpack --config=./webpack-dist.config --profile --json > stats.json",
        "build": "webpack --config=./webpack-dist.config",
        "build:dev": "webpack --config=./webpack.config",
        "cc:reportAll": "scripts/code-coverage.sh",
        "clean": "rm -rf assets coverage **/coverage cypress/screenshots cypress/videos dist node_modules stats.json .nyc_output",
        "codestyles:files": "cross-env prettier-eslint './*.js' --list-different -l silent",
        "codestyles:fix": "cross-env prettier-eslint --write",
        "codestyles:fix:all": "cross-env prettier-eslint '**/*.js' --write",
        "codestyles:fix:staged": "cross-env FILES=$(git diff --cached --name-only --diff-filter=ACM | grep \".jsx\\{0,1\\}$\"); npx prettier-eslint $FILES --write",
        "cypress:dashboard": "cypress run --record --parallel --config video=true --ci-build-id $CI_BUILD_NUMBER",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run",
        "cypress:watch": "cypress run --headed",
        "eslint": "eslint '**/*.js' --quiet",
        "eslint:fix": "eslint '**/*.js' --quiet --fix",
        "start": "webpack-dev-server",
        "start:build": "cross-env NODE_ENV=production URL=dev-homepage.library.uq.edu.au NODE_OPTIONS='--max-old-space-size=2048' webpack-dev-server --config=./webpack-dist.config --disable-host-check",
        "start:build:e2e": "cross-env NODE_ENV=production URL=localhost USE_MOCK=true NODE_OPTIONS='--max-old-space-size=2048' webpack-dev-server --config=./webpack-dist.config",
        "start:mock": "cross-env USE_MOCK=true FULL_PATH=http://localhost:2020 NODE_OPTIONS='--max-old-space-size=2048' webpack-dev-server",
        "start:devapi": "cross-env FULL_PATH=http://localhost:2020 NODE_OPTIONS='--max-old-space-size=2048' webpack-dev-server",
        "start:url": "cross-env URL=dev-homepage.library.uq.edu.au NODE_OPTIONS='--max-old-space-size=2048' webpack-dev-server",
        "test": "cross-env NODE_ENV=test FULL_PATH=http://localhost jest",
        "test:all": "npm run test:unit && npm run test:e2e && npm run test:cs -s",
        "pretest:cc": "rm -rf coverage .nyc_output",
        "test:cc": "npm run test:unit:ci && npm run test:e2e:cc && npm run cc:reportAll",
        "test:cs": "echo \"Running code style checks...\" && FILES=$(npm run codestyles:files -s) && echo \"No code style issues found.\" || echo \"File(s) failed code style checks!:\" && echo $FILES",
        "pretest:e2e": "rm -rf .nyc_output",
        "test:e2e": "start-server-and-test 'npm run start:mock' http-get://localhost:2020 'cypress run --record false'",
        "pretest:e2e:cc": "rm -rf .nyc_output coverage/cypress",
        "test:e2e:cc": "cross-env NODE_ENV=cc npm run test:e2e",
        "test:e2e:ci": "cross-env NODE_ENV=cc start-server-and-test 'npm run start:mock' http-get://localhost:2020",
        "test:e2e:ci1": "npm run test:e2e:ci 'cypress run --record false --spec $(paste -sd, bin/group1.txt)'",
        "test:e2e:ci2": "npm run test:e2e:ci 'cypress run --record false --spec $(paste -sd, bin/group2.txt)'",
        "test:e2e:ci3": "npm run test:e2e:ci 'cypress run --record false --spec $(paste -sd, bin/group3.txt)'",
        "posttest:e2e:cc": "rm -rf .nyc_output",
        "test:unit": "cross-env NODE_ENV=test FULL_PATH=http://localhost jest",
        "test:unit:serial": "npm run test:unit -- --ci --runInBand",
        "pretest:unit:ci": "rm -rf coverage",
        "test:unit:ci": "npm run test:unit -- --ci",
        "test:unit:ci:nocoverage": "npm run test:unit:ci -- --no-coverage",
        "test:unit:ci:serial:nocoverage": "npm run test:unit:ci:serial -- --no-coverage",
        "test:unit:serial:skipcoverage": "npm run test:unit -- --ci --runInBand --no-coverage",
        "test:unit:ci1:skipcoverage": "npm run test:unit -- --ci --runInBand --no-coverage --testPathIgnorePatterns=SharedComponents",
        "test:unit:ci2:skipcoverage": "npm run test:unit -- --ci --runInBand --no-coverage --passWithNoTests SharedComponents",
        "test:unit:update": "npm run test:unit -- --updateSnapshot",
        "test:unit:watch": "npm run test:unit -- --watch --onlyChanged",
        "postinstall": "vendor-copy"
    },
    "engines": {
        "npm": ">=10.5.0",
        "node": ">=18.19.0"
    },
    "babel": {
        "presets": [
            "@babel/env",
            "@babel/react"
        ],
        "env": {
            "development": {
                "presets": [
                    "@babel/env",
                    "@babel/react"
                ]
            },
            "production": {
                "presets": [
                    [
                        "env",
                        {
                            "es2015": {
                                "modules": false
                            },
                            "targets": {
                                "ie": 11,
                                "terser": true
                            }
                        }
                    ]
                ],
                "plugins": [
                    "@babel/plugin-transform-react-constant-elements",
                    "transform-react-remove-prop-types",
                    "@babel/plugin-proposal-export-default-from"
                ]
            },
            "test": {
                "presets": [
                    "@babel/env",
                    "@babel/react"
                ],
                "plugins": [
                    [
                        "dynamic-import-node",
                        {
                            "noInterop": true
                        }
                    ],
                    [
                        "@babel/plugin-transform-runtime",
                        {
                            "helpers": false,
                            "regenerator": true,
                            "corejs": false
                        }
                    ],
                    "@babel/plugin-proposal-export-default-from"
                ]
            },
            "cc": {
                "presets": [
                    "@babel/env",
                    "@babel/react"
                ],
                "plugins": [
                    [
                        "dynamic-import-node",
                        {
                            "noInterop": true
                        }
                    ],
                    [
                        "@babel/plugin-transform-runtime",
                        {
                            "helpers": false,
                            "regenerator": true,
                            "corejs": false
                        }
                    ],
                    "@babel/plugin-proposal-export-default-from",
                    "istanbul"
                ]
            }
        }
    },
    "jest": {
        "transform": {
            ".*": "<rootDir>/node_modules/babel-jest"
        },
        "testMatch": [
            "**/__tests__/**/*.[jt]s?(x)",
            "**/?(*.)+(test).[jt]s?(x)",
            "**/?(*.)+(integration).[jt]s?(x)"
        ],
        "moduleFileExtensions": [
            "js",
            "json",
            "es6"
        ],
        "unmockedModulePathPatterns": [
            "react"
        ],
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/modules/Pages/LearningResources/components/*.{js,jsx}",
            "src/modules/**/*.{js,jsx}",
            "src/modules/*.{js,jsx}",
            "!src/modules/**/*locale*.js",
            "!src/modules/*locale*.js",
            "!src/modules/App/components/pages.js",
            "!src/modules/testhelpers.js",
            "!src/modules/**/*.uqoriginal.js",
            "!src/**/{index,locale,config}.js",
            "!src/**/containers/*.{js,jsx}",
            "src/modules/HomePage/*.{js,jsx}",
            "src/actions/*.{js,jsx}",
            "src/helpers/*.{js,jsx}",
            "src/middleware/*.{js,jsx}",
            "src/reducers/*.{js,jsx}",
            "src/repositories/routes.js"
        ],
        "coverageDirectory": "coverage/jest",
        "coverageReporters": [
            "html",
            "text",
            "json"
        ],
        "coverageThreshold": {
            "global": {
                "statements": 1,
                "branches": 1,
                "functions": 1,
                "lines": 1
            }
        },
        "modulePaths": [
            "<rootDir>/src",
            "<rootDir>/utils"
        ],
        "moduleNameMapper": {
            "\\.(css|scss|sass)$": "<rootDir>/__mocks__/styles.js",
            "\\.(jpg|jpeg|svg)$": "<rootDir>/__mocks__/images.js"
        },
        "transformIgnorePatterns": [
            "<rootDir>/node_modules/"
        ],
        "setupFiles": [
            "./src/test.setup.js",
            "jest-localstorage-mock"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/jest.setup.js"
        ],
        "testEnvironment": "jsdom",
        "testEnvironmentOptions": {
            "url": "http://homepage-staging.library.uq.edu.au"
        }
    },
    "nyc": {
        "reporter": [
            "html",
            "text-summary",
            "json"
        ],
        "report-dir": "coverage/cypress",
        "temp-dir": "coverage/temp",
        "all": true,
        "include": [
            "src/modules/**/*.{js,jsx}",
            "src/modules/HomePage/*.{js,jsx}",
            "src/actions/*.{js,jsx}",
            "src/helpers/*.{js,jsx}",
            "src/middleware/*.{js,jsx}",
            "src/reducers/*.{js,jsx}",
            "src/repositories/routes.js"
        ],
        "exclude": [
            "src/**/{index,locale,config}.js",
            "src/**/containers/*.{js,jsx}",
            "src/**/*.{test,locale}.{js,jsx}",
            "src/modules/testhelpers.{js,jsx}",
            "src/modules/SharedComponents/ScrollTop/components/*.{js,jsx}",
            "src/modules/SharedComponents/Toolbox/Alert/components/*.{js,jsx}",
            "src/modules/SharedComponents/Toolbox/ScrollToTop/components/*.{js,jsx}",
            "src/modules/SharedComponents/Toolbox/helpers/*.{js,jsx}",
            "!src/modules/Pages/Admin/**",
            "!src/modules/App/**",
            "!src/modules/HomePage/**",
            "!src/modules/Pages/BookExamBooth/**",
            "!src/modules/Pages/DigitalLearningObjects/**",
            "!src/modules/Pages/PastExamPaperSearch/**",
            "!src/modules/Pages/PastExamPaperList/**",
            "!src/modules/Pages/LearningResources/**",
            "!src/modules/Pages/NotFound/**",
            "!src/modules/Pages/PaymentReceipt/**",
            "!src/modules/SharedComponents/**"
        ]
    },
    "vendorCopy": [
        {
            "from": "node_modules/@webcomponents/webcomponentsjs/custom-elements-es5-adapter.js",
            "to": "public/vendor/custom-elements-es5-adapter.js"
        },
        {
            "from": "node_modules/@webcomponents/webcomponentsjs/webcomponents-bundle.js",
            "to": "public/vendor/webcomponents-bundle.js"
        }
    ]
}
